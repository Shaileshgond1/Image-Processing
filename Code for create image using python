import cv2
import numpy as np
temple =np.zeros((800,900,3),np.uint8)
temple[:,:]=(0,0,0)
temple=cv2.rectangle(temple,(50,300),(320,400),[112,25,25],)
temple=cv2.rectangle(temple,(340,280),(430,420),[105,105,105],-1)
temple=cv2.rectangle(temple,(450,300),(740,400),[0,70,255])
temple=cv2.rectangle(temple,(430,350),(450,350),[112,25,25],3)
temple=cv2.rectangle(temple,(320,350),(340,350),[112,25,25])
temple=cv2.rectangle(temple,(50,300),(110,400),[112,25,25])
temple=cv2.rectangle(temple,(50,300),(180,400),[112,25,25])
temple=cv2.rectangle(temple,(50,300),(250,400),[112,25,25])
temple=cv2.rectangle(temple,(450,300),(520,400),[112,25,25])
temple=cv2.rectangle(temple,(450,300),(590,400),[112,25,25])
temple=cv2.rectangle(temple,(450,300),(670,400),[112,25,25])
temple=cv2.rectangle(temple,(450,300),(740,400),[112,25,25])
temple=cv2.rectangle(temple,(450,350),(740,400),[112,25,25])
temple=cv2.rectangle(temple,(340,350),(50,350),[112,25,25])
temple=cv2.rectangle(temple,(380,280),(385,200),[105,105,105],-1)
cv2.imshow("hi" ,temple)
cv2.waitKey()
cv2.destroyAllWindows()
